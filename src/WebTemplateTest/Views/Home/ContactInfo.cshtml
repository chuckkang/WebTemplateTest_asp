@model ContactInfoModel
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Contact Model Info";
}
<div style="align-content:center;">
    @ViewBag.Title
</div>
<table align="center" border="1">

    <tr><th> Contact</th><th> @Model.Name</th></tr>
    <tr><th> User Name</th><th> @Model.Address</th></tr>
    
</table>